<!DOCTYPE html>
<html lang="en">
<head>
  <title>COVID19 Dashboard</title>
  <link rel="stylesheet" href="/main.css">
</head>
<body>
  <div class="container">
    <h1>DANH SACH Dashboard</h1>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Ho va ten</th>
          <th>Gioi tinh</th>
          <th>Ngay sinh</th>
          <th>Dia chi</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%= benhnhan.Record.mabn %></td>
          <td><%= benhnhan.Record.hoten %></td>
          <td><%= benhnhan.Record.gioitinh %></td>
          <td><%= benhnhan.Record.ngaysinh %></td>
          <td><%= benhnhan.Record.diachi %></td>
        </tr>
        <% for(const bn of benhnhan) { %>
          <tr>
            <td><%= bn.Record.mabn %></td>
            <td><%= bn.Record.hoten %></td>
            <td><%= bn.Record.ngaysinh %></td>
            <td><%= bn.Record.gioitinh %></td>
            <td><%= bn.Record.diachi %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
</body>
</html>
